single_top_toolbar=false
style_vertical_toolbars=false
tint_on_mouseover=0
transparent_pcmanfm_sidepane=false
transparent_pcmanfm_view=false

[PanelButtonCommand]
frame=true
frame.element=button
frame.top=3
frame.bottom=3
frame.left=3
frame.right=3
frame.expanded.top=2
frame.expanded.bottom=2
frame.expanded.left=3
frame.expanded.right=3
interior=true
interior.element=button
indicator.size=10
text.normal.color=#5aeedc
text.focus.color=white
text.press.color=white
text.toggle.color=white
text.shadow=0
text.margin=1
text.iconspacing=4
indicator.element=arrow
text.margin.top=2
text.margin.bottom=2
text.margin.left=6
text.margin.right=15
frame.expansion=2.7font
min_height=+0.1font

[PanelButtonTool]
inherits=PanelButtonCommand

[Dock]
inherits=PanelButtonCommand
frame=true
interior=true
frame.top=2
frame.bottom=2
frame.left=2
frame.right=2

[DockTitle]
inherits=PanelButtonCommand
frame=false
interior=false

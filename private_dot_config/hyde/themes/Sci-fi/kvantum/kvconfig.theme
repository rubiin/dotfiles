highlight.color=white
inactive.highlight.color=#5aeedc
tooltip.base.color=#111e29
text.color=#6aeedc
window.text.color=#5aeedc
button.text.color=#5aeedc
disabled.text.color=#A0A0A0
tooltip.text.color=#5aeedc
highlight.text.color=#5aeedc
link.color=#5aeedc
link.visited.color=#FF6666

[Hacks]
respect_darkness=true
blur_translucent=false
transparent_dolphin_view=false
transparent_ktitle_label=false
transparent_menutitle=false
force_size_grip=true
iconless_menu=false
iconless_pushbutton=false
blur_only_active_window=false
centered_forms=false
disabled_icon_opacity=100
kinetic_scrolling=false
lxqtmainmenu_iconsize=0
middle_click_scroll=false
no_selection_tint=false
noninteger_translucency=false

respect_DE=true
saturation=1.00
scrollable_menu=true
scrollbar_in_view=false
shadowless_popup=true
spin_button_width=16
submenu_delay=250
submenu_overlap=0
tooltip_blur_radius=0
tooltip_delay=-1
transient_groove=false
tree_branch_line=false

[GeneralColors]
window.color=#070d1a
base.color=#070d1a
alt.base.color=#111e29
button.color=#070d1a
light.color=#5aeedc
mid.light.color=#5aeedc
dark.color=#5aeedc
